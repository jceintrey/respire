{
  "name": "respire-backend",
  "version": "1.0.0",
  "description": "Backend API for Respire - Cardiac Coherence App",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "migrate": "node dist/db/migrate.js",
    "migrate:down": "node dist/db/migrate.js down",
    "migrate:dev": "tsx src/db/migrate.ts",
    "migrate:dev:down": "tsx src/db/migrate.ts down",
    "seed": "node dist/db/seed.js",
    "seed:dev": "tsx src/db/seed.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@fastify/cookie": "^9.3.1",
    "@fastify/cors": "^9.0.1",
    "@fastify/jwt": "^10.0.0",
    "argon2": "^0.31.2",
    "fastify": "^4.26.2",
    "fastify-plugin": "^5.1.0",
    "google-auth-library": "^9.6.3",
    "kysely": "^0.27.3",
    "pg": "^8.11.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@types/pg": "^8.11.2",
    "pino-pretty": "^11.3.0",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  }
}
